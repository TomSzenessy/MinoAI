{
	"name": "@mino-ink/mcp-server",
	"version": "0.1.0",
	"description": "Model Context Protocol server for Mino knowledge platform",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"mino-mcp": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist",
