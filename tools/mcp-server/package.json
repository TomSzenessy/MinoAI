		"start": "node dist/cli.js"
	},
	"dependencies": {
		"@mino-ink/shared": "workspace:*",
		"@modelcontextprotocol/sdk": "^1.0.0"
	},
	"devDependencies": {
		"typescript": "^5.7.0"
	},
	"keywords": [
		"mcp",
		"model-context-protocol",
		"mino",
		"knowledge-management",
		"ai"
	],
	"author": "Mino",
	"license": "MIT"
}
